public class Palindrome_using_recursion {

	public static void main(String[] args) {
		
		int n = 454;
		int temp = n;
		
		int reverse = 0;
		int result;
		
		while(temp > 0){
			result= temp%10;
			reverse=reverse*10+result;
			temp=temp/10;
		
		}
		if(n==reverse){
			System.out.println("true");
			
		}else {
			System.out.println("false");
		}

	}

}
